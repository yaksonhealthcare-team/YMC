{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist_prod/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist_prod/index.html"
      }
    ],
    "github": {
      "enabled": true,
      "silent": true,
      "autoAlias": true
    }
  } 